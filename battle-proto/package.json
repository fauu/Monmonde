{
  "name": "monmonde-battle",
  "version": "0.1.0",
  "scripts": {
    "clean-out": "rimraf out",
    "dev": "run-s dev-server",
    "prod": "run-s build:prod",
    "type-check": "tsc --noEmit",
    "dev-server": "webpack serve --config webpack.dev.js --progress",
    "gen-barrels": "barrelsby --directory 'src' --delete --location 'all' --local && rimraf 'src/index.ts'",
    "build:dev": "run-s clean-out webpack:dev",
    "build:prod": "run-s clean-out webpack:prod",
    "webpack:dev": "webpack --config webpack.dev.js --progress",
    "webpack:prod": "webpack --config webpack.prod.js --progress",
    "workbook-to-game-data:compile": "tsc src/scripts/WorkbookToGameData.ts",
    "workbook-to-game-data:run": "node src/scripts/WorkbookToGameData.js",
    "workbook-to-game-data:clean": "rimraf src/scripts/WorkbookToGameData.js",
    "workbook-to-game-data": "run-s workbook-to-game-data:compile workbook-to-game-data:run workbook-to-game-data:clean"
  },
  "devDependencies": {
    "@babel/core": "7.17.8",
    "@babel/plugin-proposal-class-properties": "7.16.7",
    "@babel/preset-env": "7.16.11",
    "@babel/preset-react": "7.16.7",
    "@babel/preset-typescript": "7.16.7",
    "@emotion/babel-plugin": "11.7.2",
    "@pmmmwh/react-refresh-webpack-plugin": "0.5.4",
    "@types/node": "17.0.23",
    "@types/chroma-js": "2.1.3",
    "@types/react": "17.0.43",
    "@types/react-dom": "17.0.14",
    "@typescript-eslint/parser": "5.17.0",
    "@typescript-eslint/eslint-plugin": "5.17.0",
    "babel-loader": "8.2.4",
    "babel-plugin-module-resolver": "4.1.0",
    "barrelsby": "2.3.3",
    "copy-webpack-plugin": "10.2.4",
    "core-js": "3.21.1",
    "eslint": "8.12.0",
    "eslint-config-prettier": "8.5.0",
    "eslint-plugin-import": "2.25.4",
    "eslint-plugin-react": "7.29.4",
    "file-loader": "6.2.0",
    "fork-ts-checker-webpack-plugin": "7.2.2",
    "gsap": "3.10.2",
    "html-webpack-plugin": "5.5.0",
    "npm-run-all": "4.1.5",
    "prettier": "2.6.2",
    "react-refresh": "0.11.0",
    "rimraf": "3.0.2",
    "xlsx": "0.18.5",
    "tsconfig-paths-webpack-plugin": "3.5.2",
    "ts-node": "10.7.0",
    "typescript": "4.6.3",
    "url-loader": "4.1.1",
    "webpack": "5.71.0",
    "webpack-cli": "4.9.2",
    "webpack-dev-server": "4.7.4",
    "webpack-merge": "5.8.0",
    "yarn-deduplicate": "4.0.0"
  },
  "dependencies": {
    "@emotion/react": "11.8.2",
    "@emotion/styled": "11.8.1",
    "@pixi/mixin-get-child-by-name": "6.3.0",
    "chroma-js": "2.4.2",
    "pixi.js": "6.3.0",
    "react": "18.0.0",
    "react-dom": "18.0.0",
    "react-hotkeys": "2.0.0"
  },
  "private": true
}
